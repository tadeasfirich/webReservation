<p id="notice"><%= notice %></p>

<h1>Teacher Assigments</h1>

<table>
  <thead>
    <tr>
      <th>Teacher</th>
      <th>Course</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teacher_assigments.each do |teacher_assigment| %>
      <tr>
        <td><%= teacher_assigment.teacher %></td>
        <td><%= teacher_assigment.course %></td>
        <td><%= link_to 'Show', teacher_assigment %></td>
        <td><%= link_to 'Edit', edit_teacher_assigment_path(teacher_assigment) %></td>
        <td><%= link_to 'Destroy', teacher_assigment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teacher Assigment', new_teacher_assigment_path %>
